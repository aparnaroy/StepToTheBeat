<?xml version="1.0" encoding="utf-8"?>

<FrameLayout
  xmlns:android="http://schemas.android.com/apk/res/android"
  xmlns:app="http://schemas.android.com/apk/res-auto"
  xmlns:tools="http://schemas.android.com/tools"
  android:id="@+id/container_choose_pace_dm"
  android:layout_width="match_parent"
  android:layout_height="892dp">
  
  <LinearLayout
    android:id="@+id/container_set_pace"
    android:layout_width="412dp"
    android:layout_height="892dp"
    android:layout_marginTop="19dp"
    android:layout_gravity="top|left"
    android:background="@drawable/container_set_pace"
    android:gravity="left|top"
    android:orientation="vertical">
    
    <FrameLayout
      android:layout_width="match_parent"
      android:layout_height="52dp">
      
      <LinearLayout
        android:id="@+id/container_status_bar"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:clipToPadding="false"
        android:gravity="left|bottom"
        android:orientation="horizontal"
        android:paddingBottom="10dp"
        android:paddingLeft="24dp"
        android:paddingRight="24dp"
        android:paddingTop="10dp">
        
        <TextView
          android:id="@+id/text_time"
          android:layout_width="wrap_content"
          android:layout_height="20dp"
          android:gravity="left|center_vertical"
          android:includeFontPadding="false"
          android:maxLines="1"
          android:singleLine="true"
          android:text="9:30"
          android:textColor="#FF1D1B20"
          android:textSize="14dp" />
        <!-- I5:3932;50770:11430:space-view0 .Building Blocks/status-bar_space_view0 space-view -->
        <View
          android:layout_width="0dp"
          android:layout_height="20dp"
          android:layout_weight="1" />
        
        <FrameLayout
          android:id="@+id/container_right_icons"
          android:layout_width="46dp"
          android:layout_height="17dp">
          
          <ImageView
            android:id="@+id/image_wifi"
            android:layout_width="17dp"
            android:layout_height="17dp"
            android:layout_marginRight="29dp"
            android:scaleType="centerInside"
            android:src="@drawable/image_wifi" />
          
          <ImageView
            android:id="@+id/image_signal"
            android:layout_width="17dp"
            android:layout_height="17dp"
            android:layout_marginLeft="16dp"
            android:layout_marginRight="13dp"
            android:scaleType="centerInside"
            android:src="@drawable/image_signal" />
          
          <ImageView
            android:id="@+id/image_battery"
            android:layout_width="8dp"
            android:layout_height="15dp"
            android:layout_marginLeft="38dp"
            android:layout_marginTop="1dp"
            android:layout_marginBottom="1dp"
            android:scaleType="centerInside"
            android:src="@drawable/image_battery" />
        </FrameLayout>
      </LinearLayout>
      
      <ImageView
        android:id="@+id/image_camera_cutout"
        android:layout_width="24dp"
        android:layout_height="24dp"
        android:layout_marginTop="18dp"
        android:layout_gravity="top|center_horizontal"
        android:scaleType="centerInside"
        android:src="@drawable/image_camera_cutout" />
    </FrameLayout>
    
    <FrameLayout
      android:id="@+id/image_navigation"
      android:layout_width="match_parent"
      android:layout_height="24dp"
      android:layout_marginTop="816dp">
      
      <ImageView
        android:layout_width="108dp"
        android:layout_height="4dp"
        android:layout_gravity="center"
        android:background="@drawable/drawable_shape"
        android:scaleType="centerInside" />
    </FrameLayout>
  </LinearLayout>
  
  <LinearLayout
    android:id="@+id/container_content"
    android:layout_width="282dp"
    android:layout_height="86dp"
    android:layout_marginLeft="65dp"
    android:layout_marginTop="224dp"
    android:layout_gravity="top|left"
    android:background="@drawable/container_content"
    android:clipToPadding="false"
    android:gravity="left|center_vertical"
    android:orientation="horizontal"
    android:paddingBottom="12dp"
    android:paddingLeft="24dp"
    android:paddingRight="24dp"
    android:paddingTop="12dp">
    
    <TextView
      android:id="@+id/text_walk"
      android:layout_width="wrap_content"
      android:layout_height="40dp"
      android:gravity="center"
      android:includeFontPadding="false"
      android:maxLines="1"
      android:singleLine="true"
      android:text="Walk"
      android:textColor="#FFFFFFFF"
      android:textSize="32dp" />
  </LinearLayout>
  
  <LinearLayout
    android:id="@+id/container_content1"
    android:layout_width="282dp"
    android:layout_height="85dp"
    android:layout_marginLeft="65dp"
    android:layout_marginTop="340dp"
    android:layout_gravity="top|left"
    android:background="@drawable/container_content1"
    android:clipToPadding="false"
    android:gravity="left|center_vertical"
    android:orientation="horizontal"
    android:paddingBottom="12dp"
    android:paddingLeft="24dp"
    android:paddingRight="24dp"
    android:paddingTop="12dp">
    
    <TextView
      android:id="@+id/text_power_walk"
      android:layout_width="wrap_content"
      android:layout_height="40dp"
      android:gravity="center"
      android:includeFontPadding="false"
      android:maxLines="1"
      android:singleLine="true"
      android:text="Power Walk"
      android:textColor="#FFFFFFFF"
      android:textSize="32dp" />
  </LinearLayout>
  
  <LinearLayout
    android:id="@+id/container_content2"
    android:layout_width="282dp"
    android:layout_height="86dp"
    android:layout_marginLeft="65dp"
    android:layout_marginTop="455dp"
    android:layout_gravity="top|left"
    android:background="@drawable/container_content2"
    android:clipToPadding="false"
    android:gravity="left|center_vertical"
    android:orientation="horizontal"
    android:paddingBottom="12dp"
    android:paddingLeft="24dp"
    android:paddingRight="24dp"
    android:paddingTop="12dp">
    
    <TextView
      android:id="@+id/text_jog"
      android:layout_width="wrap_content"
      android:layout_height="40dp"
      android:gravity="center"
      android:includeFontPadding="false"
      android:maxLines="1"
      android:singleLine="true"
      android:text="Jog"
      android:textColor="#FFFFFFFF"
      android:textSize="32dp" />
  </LinearLayout>
  
  <LinearLayout
    android:id="@+id/container_content3"
    android:layout_width="282dp"
    android:layout_height="85dp"
    android:layout_marginLeft="65dp"
    android:layout_marginTop="572dp"
    android:layout_gravity="top|left"
    android:background="@drawable/container_content3"
    android:clipToPadding="false"
    android:gravity="left|center_vertical"
    android:orientation="horizontal"
    android:paddingBottom="12dp"
    android:paddingLeft="24dp"
    android:paddingRight="24dp"
    android:paddingTop="12dp">
    
    <TextView
      android:id="@+id/text_run"
      android:layout_width="wrap_content"
      android:layout_height="40dp"
      android:gravity="center"
      android:includeFontPadding="false"
      android:maxLines="1"
      android:singleLine="true"
      android:text="Run"
      android:textColor="#FFFFFFFF"
      android:textSize="32dp" />
  </LinearLayout>
  
  <LinearLayout
    android:id="@+id/container_content4"
    android:layout_width="282dp"
    android:layout_height="86dp"
    android:layout_marginLeft="65dp"
    android:layout_marginTop="688dp"
    android:layout_gravity="top|left"
    android:background="@drawable/container_content4"
    android:clipToPadding="false"
    android:gravity="left|center_vertical"
    android:orientation="horizontal"
    android:paddingBottom="12dp"
    android:paddingLeft="24dp"
    android:paddingRight="24dp"
    android:paddingTop="12dp">
    
    <TextView
      android:id="@+id/text_sprint"
      android:layout_width="wrap_content"
      android:layout_height="40dp"
      android:gravity="center"
      android:includeFontPadding="false"
      android:maxLines="1"
      android:singleLine="true"
      android:text="Sprint"
      android:textColor="#FFFFFFFF"
      android:textSize="32dp" />
  </LinearLayout>
  
  <LinearLayout
    android:id="@+id/container_top_app_bar"
    android:layout_width="412dp"
    android:layout_height="64dp"
    android:layout_marginTop="85dp"
    android:layout_gravity="top|left"
    android:background="@drawable/container_top_app_bar"
    android:clipToPadding="false"
    android:gravity="left|center_vertical"
    android:orientation="horizontal"
    android:paddingBottom="8dp"
    android:paddingLeft="4dp"
    android:paddingRight="4dp"
    android:paddingTop="8dp">
    
    <ImageView
      android:id="@+id/image_arrow_back"
      android:layout_width="42dp"
      android:layout_height="40dp"
      android:scaleType="centerInside"
      android:src="@drawable/image_arrow_back" />
    
    <TextView
      android:id="@+id/text_select_pace"
      android:layout_width="match_parent"
      android:layout_height="28dp"
      android:layout_marginLeft="6dp"
      android:layout_weight="1"
      android:gravity="center_horizontal|center_vertical"
      android:includeFontPadding="false"
      android:maxLines="1"
      android:singleLine="true"
      android:text="Select Pace"
      android:textColor="#FFCD9BC9"
      android:textSize="22dp" />
    
    <ImageView
      android:id="@+id/image_home"
      android:layout_width="30dp"
      android:layout_height="28dp"
      android:layout_marginLeft="6dp"
      android:scaleType="centerInside"
      android:src="@drawable/image_home" />
    
    <LinearLayout
      android:id="@+id/container_trailing_icon"
      android:layout_width="48dp"
      android:layout_height="48dp"
      android:layout_marginLeft="6dp"
      android:layout_marginTop="4dp"
      android:layout_marginBottom="4dp"
      android:background="@drawable/container_trailing_icon"
      android:gravity="center"
      android:orientation="horizontal">
      
      <ImageView
        android:id="@+id/image_state_layer"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:scaleType="centerInside"
        android:src="@drawable/image_state_layer" />
    </LinearLayout>
  </LinearLayout>
</FrameLayout>
